{
  "jsc": {
    "externalHelpers": true,
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "minify": {
      "compress": {
        "unused": true
      },
      "mangle": true,
      "ecma": 2015
    }
  },
  "minify": true,
  "env": {
    "targets": {
      "node": "14",
      "chrome": "60",
      "firefox": "60"
    }
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  }
}
